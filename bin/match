#!/usr/bin/env python
from sys import argv, stdin
import numpy as np


def main():
    q = np.loadtxt(argv[1], dtype=float, ndmin=1)
    s = np.loadtxt(stdin, dtype=float, ndmin=1)
    k, n = len(q), len(s)
    assert not k == 0
    assert not n < k
    weights = np.arange(1, k + 1)
    distances = np.array(
        [
            np.sum(weights * ((s[i : i + k] - q) ** 2))  # noqa: E203
            for i in range(n - k + 1)
        ]
    )
    best_idx = np.argmin(distances)
    print(best_idx)


if __name__ == "__main__":
    main()
