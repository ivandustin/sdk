#!/usr/bin/env python
from sys import argv, stdin
from numpy import loadtxt


def main():
    length = int(argv[1])
    indexfile = argv[2]
    indices = loadtxt(indexfile, dtype=int, ndmin=1)
    values = loadtxt(stdin, dtype=str, ndmin=1)
    for i in indices:
        start = i - length
        end = start + length
        array = values[start:end]
        assert len(array) == length
        print(" ".join(array))


if __name__ == "__main__":
    main()
