#!/usr/bin/env python
from sys import argv, stdin
from numpy import loadtxt


def main():
    length = int(argv[1])
    values = loadtxt(argv[2], dtype=str, ndmin=1)
    for line in stdin:
        i = int(line)
        start = i - length
        end = start + length
        array = values[start:end]
        assert len(array) == length
        print(" ".join(array))


if __name__ == "__main__":
    main()
